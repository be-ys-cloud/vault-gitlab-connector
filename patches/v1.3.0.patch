+				Type:        framework.TypeString,
+				Description: "Gitlab API state",
+				Default:     "",
+			},
+		},
+
+		Callbacks: map[logical.Operation]framework.OperationFunc{
+			logical.UpdateOperation: b.pathOauthLogin,
+			logical.ReadOperation:   b.pathOauthLogin,
